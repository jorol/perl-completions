    "module boilerplate" : {
       "body" : [
          "package ${1:MODULE};",
          "",
          "use strict;",
          "use warnings;",
          "use 5.008_005;",
          "our \\$VERSION = '0.01';",
          "",
          "sub new {",
          "\tmy(\\$class, %args) = @_;",
          "\tmy \\$self = bless({}, \\$class);",
          "\treturn \\$self;",
          "}",
          "",
          "1;",
          "",
          "__END__",
          "",
          "=encoding utf-8",
          "",
          "=head1 NAME",
          "",
          "${2:MODULE} - Blah blah blah",
          "",
          "=head1 SYNOPSIS",
          "",
          "  use ${3:MODULE};",
          "",
          "=head1 DESCRIPTION",
          "",
          "${4:MODULE} is ...",
          "",
          "=head2 Methods",
          " ",
          "=head3 new",
          "",
          "I<new> does ...",
          "",
          "=head1 AUTHOR",
          "",
          "${5:AUTHOR} E<lt>${6:AUTHOREMAIL}E<gt>",
          "",
          "=head1 CONTRIBUTORS",
          "",
          "${7:CONTRIBUTOR} E<lt>${8:CONTRIBUTOREMAIL}E<gt>",
          "",
          "=head1 COPYRIGHT",
          "",
          "Copyright 2021- ${9:AUTHOR}",
          "",
          "=head1 LICENSE",
          "",
          "This library is free software; you can redistribute it and/or modify",
          "it under the same terms as Perl itself.",
          "",
          "=head1 SEE ALSO",
          "",
          "L<${10:MODULE}>",
          "",
          "=cut"
       ],
       "description" : "module boilerplate",
       "prefix" : "init module"
    },
